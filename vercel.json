{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/next" },
    { "src": "api/publish.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/publish", "dest": "/api/publish.ts" }
  ],
  "crons": [
    { "path": "/api/cron/instagram-post", "schedule": "0 14 * * *" }
  ]
}
